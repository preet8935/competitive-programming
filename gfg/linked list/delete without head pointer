void deleteNode(Node *node)
{
   // Your code here
   if(node == NULL) return ;
   else
   {
       if(node->next == NULL) return;
       
       else
       {
           Node* temp=node->next;
           node->data = node->next->data;
           node->next = node->next->next;
           delete temp;
       }
   }
}
